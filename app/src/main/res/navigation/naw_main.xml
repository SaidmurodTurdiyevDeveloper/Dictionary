<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/naw_main"
    app:startDestination="@id/fragmentSplash">
    <fragment
        android:id="@+id/fragmentSplash"
        android:name="com.example.dictionary.ui.screens.FragmentSplash"
        android:label="FragmentSplash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/open_choose_cauntry"
            app:destination="@id/fragmentChooseLanguages"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/fragmentSplash"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/open_main"
            app:destination="@id/fragmentMain"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/fragmentSplash"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/fragmentMain"
        android:name="com.example.dictionary.ui.screens.dictionary.FragmentMain"
        android:label="FragmentMain"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/open_game"
            app:destination="@id/fragmentGame"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_setting"
            app:destination="@id/fragmentSetting"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_choose_cauntry_two"
            app:destination="@id/fragmentChooseLanguages"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />

        <action
            android:id="@+id/open_app_info"
            app:destination="@id/fragmentAppInfo"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_dictionary_itm"
            app:destination="@id/fragmentDictionaryItem"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_dictionary_words"
            app:destination="@id/fragmentWordsLearn"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_archive"
            app:destination="@id/fragmentArchive"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragmentGame"
        android:name="com.example.dictionary.ui.screens.FragmentGame"
        android:label="FragmentGame"
        tools:layout="@layout/fragment_game" />
    <fragment
        android:id="@+id/fragmentSetting"
        android:name="com.example.dictionary.ui.screens.FragmentSetting"
        android:label="FragmentSetting"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/fragmentChooseLanguages"
        android:name="com.example.dictionary.ui.screens.FragmentChooseLanguages"
        android:label="FragmentChooseLanguages"
        tools:layout="@layout/fragment_select_country">
        <action
            android:id="@+id/open_main_two"
            app:destination="@id/fragmentMain"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/fragmentChooseLanguages"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/fragmentWordsLearn"
        android:name="com.example.dictionary.ui.screens.words.FragmentWordsLearn"
        android:label="FragmentWordsLearn"
        tools:layout="@layout/fragment_words_learn">
        <action
            android:id="@+id/open_words_play"
            app:destination="@id/fragmentWordsPlay"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_words_list"
            app:destination="@id/fragmentWordsList"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="Id"
            app:argType="long" />
        <action
            android:id="@+id/open_words_item"
            app:destination="@id/wordItem"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragmentWordsList"
        android:name="com.example.dictionary.ui.screens.words.FragmentWordsList"
        android:label="FragmentWordsList"
        tools:layout="@layout/fragment_words_list">
        <action
            android:id="@+id/open_words_item_two"
            app:destination="@id/wordItem"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="Id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/wordItem"
        android:name="com.example.dictionary.ui.screens.words.FragmentWordItem"
        android:label="WordItem"
        tools:layout="@layout/fragment_word_item" >
        <argument
            android:name="Id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAppInfo"
        android:name="com.example.dictionary.ui.screens.FragmentAppInfo"
        android:label="FragmentAppInfo"
        tools:layout="@layout/fragment_app_info" />
    <fragment
        android:id="@+id/fragmentDictionaryInfo"
        android:name="com.example.dictionary.ui.screens.dictionary.FragmentDictionaryInfo"
        android:label="FragmentDictionaryInfo"
        tools:layout="@layout/fragment_dictionary_info" >
        <argument
            android:name="Id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/fragmentWordsPlay"
        android:name="com.example.dictionary.ui.screens.words.FragmentWordsPlay"
        android:label="FragmentWordsPlay"
        tools:layout="@layout/fragment_words_play" >
        <argument
            android:name="Id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/fragmentDictionaryItem"
        android:name="com.example.dictionary.ui.screens.dictionary.FragmentDictionaryItem"
        android:label="FragmentDictionaryItem"
        tools:layout="@layout/fragment_dictionary_item">
        <action
            android:id="@+id/open_words_list_two"
            app:destination="@id/fragmentWordsList"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/open_dictionary_info"
            app:destination="@id/fragmentDictionaryInfo"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="Id"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/fragmentArchive"
        android:name="com.example.dictionary.ui.screens.dictionary.FragmentArchive"
        android:label="FragmentArchive"
        tools:layout="@layout/fragment_archive" />
</navigation>